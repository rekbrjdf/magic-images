{
  "name": "mini-app",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env PORT=10888 HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "vk-miniapps-deploy",
    "tunnel": "vk-tunnel --insecure=1 --http-protocol=https --ws-protocol=wss --host=0.0.0.0 --port=10888",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "lint": "eslint src/",
    "lint:write": "eslint src/ --fix",
    "stylelint": "stylelint --fix --config .stylelintrc.js 'src/**/*.{scss,css}'"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ],
    "*.{html,scss,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@vkontakte/icons": "^2.7.0",
    "@vkontakte/vk-bridge": "latest",
    "@vkontakte/vk-mini-apps-router": "^1.3.1",
    "@vkontakte/vk-miniapps-deploy": "0.0.25",
    "@vkontakte/vk-tunnel": "^0.1.4",
    "@vkontakte/vkui": "5.1.0",
    "axios": "^1.6.0",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.3.2",
    "eruda": "^2.4.1",
    "eruda-code": "^2.0.0",
    "eruda-dom": "^2.0.0",
    "git": "^0.1.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.1.3",
    "react-router-vkminiapps": "^1.3.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^6.0.0",
    "jest": "^26.6.0",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "nodegit": "^0.27.0",
    "prettier": "^2.3.0",
    "react-hot-loader": "^4.13.0",
    "react-scripts": "^5.0.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^5.1.0"
  },
  "resolutions": {
    "react-scripts/webpack-dev-server/yargs/yargs-parser": ">=18.1.2"
  },
  "overrides": {
    "es5-ext@^0.10.50": "0.10.53"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
